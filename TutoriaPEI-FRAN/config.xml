<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.pei.fran" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TutoriaPEI-FRAN</name>
    <description>App de tutorias da escola Francisca Ribeiro</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="*"/>
    <preference name="scheme" value="http"/>
    <icon src="res/logo-escola.png" />
</widget>
